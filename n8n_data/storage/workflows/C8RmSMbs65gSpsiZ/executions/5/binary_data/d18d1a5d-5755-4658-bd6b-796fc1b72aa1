#  attractions
- created_at datetime(6) 
- updated_at datetime(6) 
- attraction_id uuid 
- attraction_code varchar(50) 
- attraction_name varchar(200) 
- country_code varchar(2) 
- city_name varchar(100) 
- region varchar(100) 
- address longtext 
- category varchar(50) 
- subcategory varchar(50) 
- tags longtext 
- description longtext 
- highlights longtext 
- recommended_duration int(11) 
- opening_hours longtext 
- best_season longtext 
- is_always_open tinyint(1) 
- ticket_price decimal(10,2) 
- currency varchar(3) 
- ticket_type varchar(50) 
- booking_required tinyint(1) 
- booking_website varchar(500) 
- facilities longtext 
- popularity_score decimal(3,2) 
- visitor_rating decimal(3,2) 
- review_count int(11) 
- main_image_url varchar(500) 
- image_gallery longtext 
- video_url varchar(500) 
- status varchar(20) 
- created_by varchar(50) 
- updated_by varchar(50) 
- version int(11) 

